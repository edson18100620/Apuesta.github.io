<div class="alert alert-primary">
    <div class="row">
        <div class="col-md-4">
            <h1>Mantenimiento de AudioVisual</h1>
        </div>
        <div class="col-md-2">
            <button id="btnAgregarAudioVisual" class="btn btn-success">
                Agregar  
            </button>
        </div>
    </div>
</div>

<div id="divListadoAudioVisual" class="divListadoAudioVisual">

</div>

<script>

    CargarListadoAudioVisual()
    function CargarListadoAudioVisual()
    {

        $.ajax({

            type: "GET",
            url: "@Url.Action("Listado","AudioVisual")",
            cache: false,
            success: function(dataHTMLAudioVisual) {
                $("#divListadoAudioVisual").html(dataHTMLAudioVisual)
            }
        });
    }
</script>